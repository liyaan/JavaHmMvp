<?xml version="1.0" encoding="utf-8"?>
<DirectionalLayout
    xmlns:ohos="http://schemas.huawei.com/res/ohos"
    ohos:height="match_content"
    ohos:width="match_parent"
    ohos:orientation="vertical">


    <StackLayout
        ohos:height="match_content"
        ohos:width="match_parent"
        ohos:top_margin="8vp"
        ohos:bottom_margin="8vp">

        <DirectionalLayout
            ohos:id="$+id:header"
            ohos:height="match_content"
            ohos:width="match_parent"
            ohos:left_margin="16vp"
            ohos:right_margin="16vp"
            ohos:orientation="horizontal">

            <Image
                ohos:id="$+id:prev"
                ohos:height="match_content"
                ohos:width="40vp"
                ohos:image_src="$media:ic_arrow_left_normal"/>

            <Text
                ohos:id="$+id:title"
                ohos:height="match_parent"
                ohos:width="0vp"
                ohos:text_alignment="center"
                ohos:text_size="20fp"
                ohos:weight="1"/>

            <Image
                ohos:id="$+id:next"
                ohos:height="match_content"
                ohos:width="40vp"
                ohos:image_src="$media:ic_arrow_right_normal"/>

        </DirectionalLayout>

        <Text
            ohos:id="$+id:selection_title"
            ohos:height="match_content"
            ohos:width="match_parent"
            ohos:min_height="48vp"
            ohos:text_alignment="center"
            ohos:text_color="#ffffff"
            ohos:text_size="20fp"
            ohos:visibility="hide"/>
    </StackLayout>

